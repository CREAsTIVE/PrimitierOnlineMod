{
  "Port": 20000,
  "MaxPlayers": 16,
  "Seed": 0,
  "Serilog": {
    "Using": [
      "Serilog.Settings.Configuration",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "Path": "Logs/.log",
          "RollingInterval": "Day"
        }
      }
    ]
  }
}
