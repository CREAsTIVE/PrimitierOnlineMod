{
  "name": "DevServer",
  "description": "POMテストのためのサーバー",
  "version": "0.0.0",
  "ip": "",
  "port": 54162,
  "maxPlayer": 15,
  "Serilog": {
    "Using": [
      "Serilog.Settings.Configuration",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt" }
      }
    ]
  }
}
