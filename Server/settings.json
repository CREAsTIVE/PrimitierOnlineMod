{
  "name": "DevServer",
  "description": "POMテストのためのサーバー",
  "version": "0.0.0",
  "tcpPort": 54162,
  "udpPort": 54163,
  "maxPlayer": 15,
  "Serilog": {
    "Using": [
      "Serilog.Settings.Configuration",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/.log",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
